node {
    stage('ssh to cli') {
        sh 'ssh root@cli '/sbin/httpd -k start''
    }
    stage('start httpd') {
        sh 'ssh root@cli 'service httpd status''
    }
    stage('') {
        sh 'curl localhost | mail -s "Test " root'
    }
    stage('Disk Usage') {
        sh 'df -hPT . | mail -s "DiskUsage " root'
    }

}
