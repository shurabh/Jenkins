node {
    stage('ssh to cli') {
        sh 'su - root'
        sh 'ssh root@cli "uptime"'
    }
    stage('start httpd') {
        sh 'ssh root@cli "date"'
    }
    stage('') {
        sh 'curl localhost | mail -s "Test " root'
    }
    stage('Disk Usage') {
        sh 'df -hPT . | mail -s "DiskUsage " root'
    }

}
